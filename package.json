    "babel-preset-react": "^6.24.1",
    "express": "^4.16.3",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.3.4",
    "nodemon": "^1.18.4",
    "pass": "^0.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
